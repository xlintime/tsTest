  },
  "homepage": "https://github.com/xlintime/tsTest#readme",
  "devDependencies": {
    "@types/node": "^14.14.41",
    "eslint": "^7.24.0",
    "prettier": "^2.2.1",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.4"
  }
